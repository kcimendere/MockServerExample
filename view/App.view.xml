<mvc:View controllerName="kerem.test.Test.controller.App" xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<ComboBox items="{/ProductSet}">
							<core:Item key="{ProductID}" text="{Name}"/>
						</ComboBox>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>